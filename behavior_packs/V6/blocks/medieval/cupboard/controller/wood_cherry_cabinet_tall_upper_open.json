			{"condition": "q.block_property('medieval:facing_direction') == 2","components": {"minecraft:rotation": [0,90,0]}},
			{"condition": "q.block_property('medieval:facing_direction') == 3","components": {"minecraft:rotation": [0,-90,0]}},
			{"condition": "q.block_property('medieval:side') == false","components": {"minecraft:geometry": "geometry.cabinet_tall_open_tops"}},
			{"condition": "q.block_property('medieval:side') == false && q.block_property('medieval:item') == 1","components": {"minecraft:geometry": "geometry.cabinet_tall_book_tops"}},
			{"condition": "q.block_property('medieval:side') == true && q.block_property('medieval:item') == 1","components": {"minecraft:geometry": "geometry.cabinet_tall_book_top"}}
		],
		"components": {
			"tag:medieval_furniture": {},
			"minecraft:geometry": "geometry.cabinet_tall_open_top",
			"minecraft:material_instances": {"planks": {"texture": "m.cherry_planks","render_method": "opaque"},"metal": {"texture": "light_gray_concrete","render_method": "opaque"},"*": {"texture": "m.wood_cherry","render_method": "opaque"},"book": {"texture": "book_brown","render_method": "opaque"}},
			"minecraft:map_color": "#fcd7f9",
			"tag:cabinet":{},
			"tag:cabinet_open": {},
			"minecraft:loot": "empty",
			"minecraft:placement_filter": {"conditions": [{"allowed_faces": ["up"],"block_filter": ["cherry_wood_cabinet_tall"]}]},
			"minecraft:on_placed":{"event": "rotation"},
			"minecraft:creative_category": {"category": "commands","group": "itemGroup.name.unobtainableBlocks"},
			"minecraft:on_interact": {"event": "open","target": "self"},
			"minecraft:breathability": "air",
			"minecraft:block_light_absorption": 0.0,
			"minecraft:explosion_resistance": 3.0,
			"minecraft:destroy_time": 0.8
		},

					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'side')","set_block_property": {"medieval:side": false}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'cabinet_south')","set_block_property": {"medieval:facing_direction": 1}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'cabinet_east')","set_block_property": {"medieval:facing_direction": 2}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'cabinet_west')","set_block_property": {"medieval:facing_direction": 3}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'book')","set_block_property": {"medieval:item": 1}}
				]

{
	"format_version": "1.16.100",
	"minecraft:block": {
		"description": {
			"identifier": "medieval:jungle_wood_drawer0",
			"properties": {
				"medieval:facing_direction": [0,1,2,3,4,5],
				"medieval:open": [0,1,2,3,4,5]
			}
		},
		"permutations": [
			{"condition": "q.block_property('medieval:facing_direction') == 2","components": {"minecraft:rotation": [0,0,0],"tag:junglew_table_north": {}}},
			{"condition": "q.block_property('medieval:facing_direction') == 3","components": {"minecraft:rotation": [0,180,0],"tag:junglew_table_south": {}}},
			{"condition": "q.block_property('medieval:facing_direction') == 4","components": {"minecraft:rotation": [0,90,0],"tag:junglew_table_east": {}}},
			{"condition": "q.block_property('medieval:facing_direction') == 5","components": {"minecraft:rotation": [0,-90,0],"tag:junglew_table_west": {}}},
			{"condition": "query.block_property('medieval:open') == 2","components": {"minecraft:geometry": "geometry.desktop_0_open1","minecraft:on_placed": {"event": "empty"}}},
			{"condition": "query.block_property('medieval:open') == 3","components": {"minecraft:geometry": "geometry.desktop_0_open2","minecraft:on_placed": {"event": "empty"}}},
			{"condition": "query.block_property('medieval:open') == 4","components": {"minecraft:geometry": "geometry.desktop_0_open3","minecraft:on_placed": {"event": "empty"}}},
			{"condition": "query.block_property('medieval:open') == 5","components": {"minecraft:geometry": "geometry.desktop_0_open4","minecraft:on_placed": {"event": "empty"}}}
		],
		"components": {
			"tag:medieval_furniture": {},
			"minecraft:geometry": "geometry.desktop_0_close",
			"minecraft:material_instances": {"*": {"texture": "jungle_wood","render_method": "alpha_test"},"line": {"texture": "light_gray_concrete","render_method": "alpha_test"},"paper": {"texture": "paper_item","render_method": "alpha_test"},"planks": {"texture": "jungle_planks","render_method": "alpha_test"}},
			"minecraft:creative_category": {"category": "items","group": "itemGroup.name.jungleWood"},
			"minecraft:on_player_placing": {"event": "set_facing_direction"},
			"minecraft:on_placed": {"event": "container","target": "self"},
			"minecraft:on_interact": {"event": "open"},
			"minecraft:entity_collision": {"origin": [-8.0,0.0,-8.0],"size": [16,16,16]},
			"minecraft:pick_collision": {"origin": [-8.0,0.0,-8.0],"size": [16,16,16]},
			"minecraft:block_light_absorption": 0.0,
			"minecraft:explosion_resistance": 3.0,
			"minecraft:destroy_time": 0.8,
			"minecraft:map_color": "#e39f6f"
		},
		"events": {
			"set_facing_direction": {"set_block_property": {"medieval:facing_direction": "query.cardinal_facing_2d"}},
			"container": {"sequence": [{"condition": "query.block_property('medieval:open') == 0","run_command": {"command": "summon medieval:drawer_container ~ ~ ~"},"set_block_property": {"medieval:open": 1}}]},
			"open": {
				"sequence": [
					{"condition": "query.block_property('medieval:open') < 5","run_command": {"command": "playsound block.barrel.open @a ~ ~ ~"},"set_block_property": {"medieval:open": "query.block_property('medieval:open') +1"}},
					{"condition": "query.block_property('medieval:open') == 5","run_command": {"command": "playsound block.barrel.close @a ~ ~ ~"},"set_block_property": {"medieval:open": 1}}
				]
			}
		}
	}
}
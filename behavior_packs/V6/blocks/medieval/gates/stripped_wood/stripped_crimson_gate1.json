{
	"format_version": "1.16.100",
	"minecraft:block": {
		"description": {
			"identifier": "medieval:stripped_crimson_gate1",
			"properties": {
				"medieval:facing_direction": [0,1,2,3,4,5],
				"medieval:state": [0,1,2],
				"medieval:close": [true,false]
			}
		},
		"permutations": [
			{"condition": "q.block_property('medieval:facing_direction') == 2","components": {"minecraft:rotation": [0,0,0]}},
			{"condition": "q.block_property('medieval:facing_direction') == 3","components": {"minecraft:rotation": [0,180,0]}},
			{"condition": "q.block_property('medieval:facing_direction') == 4","components": {"minecraft:rotation": [0,90,0]}},
			{"condition": "q.block_property('medieval:facing_direction') == 5","components": {"minecraft:rotation": [0,-90,0]}},
			{"condition": "q.block_property('medieval:state') == 0 && q.block_property('medieval:close') == true","components": {"minecraft:geometry": "geometry.fence_gate2_close0"}},
			{"condition": "q.block_property('medieval:state') == 1 && q.block_property('medieval:close') == true","components": {"minecraft:geometry": "geometry.fence_gate2_close1"}},
			{"condition": "q.block_property('medieval:state') == 2 && q.block_property('medieval:close') == true","components": {"minecraft:geometry": "geometry.fence_gate2_close2"}},
			{"condition": "q.block_property('medieval:state') == 0 && q.block_property('medieval:close') == false","components": {"minecraft:geometry": "geometry.fence_gate2_open0","minecraft:entity_collision": false}},
			{"condition": "q.block_property('medieval:state') == 1 && q.block_property('medieval:close') == false","components": {"minecraft:geometry": "geometry.fence_gate2_open1","minecraft:entity_collision": false}},
			{"condition": "q.block_property('medieval:state') == 2 && q.block_property('medieval:close') == false","components": {"minecraft:geometry": "geometry.fence_gate2_open2","minecraft:entity_collision": false}}
		],
		"components": {
			"tag:medieval_furniture": {},
			"minecraft:geometry": "geometry.fence_gate2_close0",
			"minecraft:material_instances": {"*": {"texture": "stripped_crimson","render_method": "opaque"}},
			"minecraft:on_player_placing": {"event": "set_facing_direction"},
			"minecraft:creative_category": {"category": "construction","group": "itemGroup.name.fenceGate"},
			"minecraft:on_interact": {"event": "open"},
			"tag:wood":{},
			"tag:fence_gate":{},
			"minecraft:ticking": {"on_tick":{"event": "update"},"range": [0,0],"looping": true},
			"minecraft:entity_collision": {"origin": [-8.0,0.0,-2.0],"size": [16,16,4]},
			"minecraft:pick_collision": {"origin": [-8.0,0.0,-2.0],"size": [16,16,4]},
			"minecraft:block_light_absorption": 0.0,
			"minecraft:explosion_resistance": 3.0,
			"minecraft:destroy_time": 0.8,
			"minecraft:map_color": "#b3244a"
		},
		"events": {
			"set_facing_direction": {"set_block_property": {"medieval:facing_direction": "q.cardinal_facing_2d"}},
			"update": {
				"sequence": [
					{"condition": "(q.block_neighbor_has_any_tag(-1,0,0,'fence_gate') && q.block_property('medieval:facing_direction') == 3) || (q.block_neighbor_has_any_tag(1,0,0,'fence_gate') && q.block_property('medieval:facing_direction') == 2) || (q.block_neighbor_has_any_tag(0,0,-1,'fence_gate') && q.block_property('medieval:facing_direction') == 4) || (q.block_neighbor_has_any_tag(0,0,1,'fence_gate') && q.block_property('medieval:facing_direction') == 5) && q.block_property('medieval:state') == 0","set_block_property": {"medieval:state": 1}},
					{"condition": "(q.block_neighbor_has_any_tag(1,0,0,'fence_gate') && q.block_property('medieval:facing_direction') == 3) || (q.block_neighbor_has_any_tag(-1,0,0,'fence_gate') && q.block_property('medieval:facing_direction') == 2) || (q.block_neighbor_has_any_tag(0,0,1,'fence_gate') && q.block_property('medieval:facing_direction') == 4) || (q.block_neighbor_has_any_tag(0,0,-1,'fence_gate') && q.block_property('medieval:facing_direction') == 5) && q.block_property('medieval:state') == 0","set_block_property": {"medieval:state": 2}},
					{"condition": "((!q.block_neighbor_has_any_tag(-1,0,0,'fence_gate') && !q.block_neighbor_has_any_tag(1,0,0,'fence_gate')) && (q.block_property('medieval:facing_direction') == 3 || q.block_property('medieval:facing_direction') == 2)) || ((!q.block_neighbor_has_any_tag(0,0,-1,'fence_gate') && !q.block_neighbor_has_any_tag(0,0,1,'fence_gate')) && (q.block_property('medieval:facing_direction') == 4 || q.block_property('medieval:facing_direction') == 5))","set_block_property": {"medieval:state": 0}}
				]
			},
			"open": {
				"sequence": [
					{"condition": "q.block_property('medieval:close') == true","set_block_property": {"medieval:close": false},"run_command":{"command":["playsound random.door_open @a ~ ~ ~"]}},
					{"condition": "q.block_property('medieval:close') == false","set_block_property": {"medieval:close": true},"run_command":{"command":["playsound random.door_close @a ~ ~ ~"]}}
				]
			}
		}
	}
}
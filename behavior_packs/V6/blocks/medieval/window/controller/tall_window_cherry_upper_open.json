{
	"format_version": "1.16.100",
	"minecraft:block": {
		"description": {
			"identifier": "medieval:tall_window_cherry_upper_open",
			"properties": {
				"medieval:direction": [0,1,2,3],
				"medieval:side": [true,false]
			}
		},
		"permutations": [
			{"condition": "q.block_property('medieval:direction') == 0","components": {"minecraft:rotation": [0,0,0]}},
			{"condition": "q.block_property('medieval:direction') == 1","components": {"minecraft:rotation": [0,180,0]}},
			{"condition": "q.block_property('medieval:direction') == 2","components": {"minecraft:rotation": [0,90,0]}},
			{"condition": "q.block_property('medieval:direction') == 3","components": {"minecraft:rotation": [0,-90,0]}},
			{"condition": "q.block_property('medieval:side') == true","components": {"minecraft:geometry": "geometry.m.tall_window_top_open_0"}},
			{"condition": "q.block_property('medieval:side') == false","components": {"minecraft:geometry": "geometry.m.tall_window_top_open_1","minecraft:entity_collision": {"origin": [-8,0,-8],"size": [4,16,10]},"minecraft:pick_collision": {"origin": [-8,0,-8],"size": [4,16,10]}}}
		],
		"components": {
			"minecraft:geometry": "geometry.m.tall_window_top_open_0",
			"minecraft:material_instances": {"*": {"texture": "m.stripped_cherry","render_method": "alpha_test","ambient_occlusion": false},"glass": {"texture": "glass","render_method": "alpha_test","ambient_occlusion": false},"knob": {"texture": "light_gray_concrete","render_method": "alpha_test","ambient_occlusion": false}},
			"minecraft:map_color": "#ffc4f8",
			"tag:tall_window":{},
			"tag:tall_window_open": {},
			"tag:medieval_furniture": {},
			"minecraft:loot": "empty",
			"minecraft:creative_category": {"category": "commands","group": "itemGroup.name.unobtainableBlocks"},
			"minecraft:placement_filter": {"conditions": [{"allowed_faces": ["up"],"block_filter": ["stripped_cherry_tall_window"]}]},
			"minecraft:on_placed":{"event": "rotation"},
			"minecraft:on_interact": {"event": "open","target": "self"},
			"minecraft:breathability": "air",
			"minecraft:entity_collision": {"origin": [4,0,-8],"size": [4,16,10]},
			"minecraft:pick_collision": {"origin": [4,0,-8],"size": [4,16,10]},
			"minecraft:block_light_absorption": 1.0,
			"minecraft:block_light_emission": 0.0,
			"minecraft:explosion_resistance": 3.0,
			"minecraft:destroy_time": 0.8
		},
		"events": {
			"rotation": {
				"sequence": [
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'side')","set_block_property": {"medieval:side": false}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'tall_window_south')","set_block_property": {"medieval:direction": 1}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'tall_window_east')","set_block_property": {"medieval:direction": 2}},
					{"condition": "q.block_neighbor_has_any_tag(0,-1,0,'tall_window_west')","set_block_property": {"medieval:direction": 3}}
				]
			},
			"open": {
				"sequence": [
					{"condition": "q.block_property('medieval:direction') == 0 && q.block_property('medieval:side') == true","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=2,\"medieval:side\"=true,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 1 && q.block_property('medieval:side') == true","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=3,\"medieval:side\"=true,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 2 && q.block_property('medieval:side') == true","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=4,\"medieval:side\"=true,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 3 && q.block_property('medieval:side') == true","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=5,\"medieval:side\"=true,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 0 && q.block_property('medieval:side') == false","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=2,\"medieval:side\"=false,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 1 && q.block_property('medieval:side') == false","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=3,\"medieval:side\"=false,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 2 && q.block_property('medieval:side') == false","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=4,\"medieval:side\"=false,\"medieval:open\"=1]"]}},
					{"condition": "q.block_property('medieval:direction') == 3 && q.block_property('medieval:side') == false","run_command":{"command":["setblock ~ ~-1 ~ medieval:stripped_cherry_tall_window [\"medieval:direction\"=5,\"medieval:side\"=false,\"medieval:open\"=1]"]}},
					{"run_command":{"command":["playsound close.cherry_wood_door @a ~ ~ ~","setblock ~ ~ ~ medieval:tall_window_cherry_upper_close"]}}
				]
			}
		}
	}
}
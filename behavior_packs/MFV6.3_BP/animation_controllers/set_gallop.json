{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.set_gallop": {
			"states": {
				"default": {
					"transitions": [
						{
							"remove_gallop": "!q.ground_speed"
						}
					]
				},
				"remove_gallop": {
					"transitions": [
						{
							"default": "(1.0)"
						}
					],
					"on_entry": [
						"@s medieval:remove_gallop"
					]
				}
			}
		}
	}
}